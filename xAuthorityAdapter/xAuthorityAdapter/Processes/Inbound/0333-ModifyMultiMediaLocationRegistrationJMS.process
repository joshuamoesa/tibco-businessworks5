<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx="http://xmlns.example.com/unique/default/namespace/1137414045923" xmlns:ns1="http://xmlns.kpn.com/common/cdm/Base.xsd" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:pfx2="http://xmlns.tibco.com/2003/5/bw/plugins/tibrv" xmlns:ns3="http://www.tibco.com/pe/EngineTypes" xmlns:ns4="http://xmlns.kpn.com/eai/common/errors" xmlns:pfx3="http://xmlns.kpn.com/fixed/common/test/response/TestRequestService" xmlns:ns5="http://xmlns.kpn.com/common/cdm/ResultStatus.xsd" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://xmlns.kpn.com/fixed/common/test/notify/TestNotificationService" xmlns:eai="java://com.kpn.eai.component.common.CommonXPathFunctions" xmlns:ns6="http://xmlns.kpn.com/fixed/xAuthorityAdapter/ModifyMultiMediaLocationRegistration.xsd" xmlns:pfx5="http://xmlns.kpn.com/Outbound/Publish/SubmitOrder.xsd" xmlns:ns7="http://xmlns.kpn.com/imported/ordermessage.xsd" xmlns:ns8="http://xmlns.kpn.com/Inbound/Subscribe/UpdateOrderStatus.xsd" xmlns:ns9="http://xmlns.kpn.com/imported/ordermessage_Site_Enterprise.xsd" xmlns:ns10="http://java.sun.com/products/oss/xml/ServiceActivation" xmlns:ns11="http://java.sun.com/products/oss/xml/Common" xmlns:ns12="http://Enterprise.BVoIPOSSJ.VoIP.BuildingBlocks.ServiceLibrary" xmlns:ns13="http://Site.BVoIPOSSJ.VoIP.BuildingBlocks.ServiceLibrary">
    <xsd:import namespace="http://xmlns.example.com/unique/default/namespace/1137414045923" schemaLocation="/Processes/Test Resources/TestPublish.xsd"/>
    <xsd:import namespace="http://xmlns.tibco.com/2003/5/bw/plugins/tibrv" schemaLocation="/Common/Shared Resources/RawRVMsg2.xsd"/>
    <xsd:import namespace="http://xmlns.kpn.com/fixed/common/test/response/TestRequestService" schemaLocation="/Common/Test Resources/XSDSchemas/TestComponent2/Response/TestRequestService.xsd"/>
    <xsd:import namespace="http://xmlns.kpn.com/fixed/common/test/notify/TestNotificationService" schemaLocation="/Common/Test Resources/XSDSchemas/TestComponent2/Notify/TestNotificationService.xsd"/>
    <xsd:import namespace="http://xmlns.kpn.com/fixed/xAuthorityAdapter/ModifyMultiMediaLocationRegistration.xsd" schemaLocation="/XSDSchemas/xAuthorityAdapter/ModifyMultiMediaLocationRegistration.xsd"/>
    <xsd:import namespace="http://xmlns.kpn.com/Outbound/Publish/SubmitOrder.xsd" schemaLocation="/XSDSchemas/xAuthorityAdapter/Publish/SubmitOrder.xsd"/>
    <xsd:import namespace="http://Enterprise.BVoIPOSSJ.VoIP.BuildingBlocks.ServiceLibrary" schemaLocation="/XSDSchemas/xAuthority/BVoIPEnterpriseDataServiceValue.xsd"/>
    <xsd:import namespace="http://Site.BVoIPOSSJ.VoIP.BuildingBlocks.ServiceLibrary" schemaLocation="/XSDSchemas/xAuthority/BVoIPSiteDataServiceValue.xsd"/>
    <pd:name>xAuthorityAdapter/Processes/Inbound/0333-ModifyMultiMediaLocationRegistrationJMS.process</pd:name>
    <pd:startName>Receive JMS Request</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Receive JMS Request">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>77</pd:x>
        <pd:y>137</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>1</acknowledgeMode>
                <maxSessions>%%Common/JMS/JmsMaxSessions%%</maxSessions>
                <destination>%%Env%%.FIXED.%%xAuthorityAdapter/ComponentDomain%%.%%xAuthorityAdapter/ComponentId%%.Request.ModifyMultiMediaLocationRegistration.1</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/xAuthorityAdapter/Shared Resources/JMS_PUBLIC Connection.sharedjmscon</ConnectionReference>
            <ApplicationProperties>/Common/Shared Resources/JMS Default Properties.sharedjmsapp</ApplicationProperties>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>653</pd:endX>
    <pd:endY>126</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:activity name="Process Request">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>427</pd:x>
        <pd:y>128</pd:y>
        <config>
            <processName>/xAuthorityAdapter/Processes/Inbound/Main_0333-ModifyMultiMediaLocationRegistration.process</processName>
        </config>
        <pd:inputBindings>
            <pfx5:SubmitOrder>
                <xsl:copy-of select="$Parse-XML/pfx5:SubmitOrder/ns1:CMH"/>
                <xsl:copy-of select="$Parse-XML/pfx5:SubmitOrder/ns7:BODY"/>
            </pfx5:SubmitOrder>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log JMS Request">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>220</pd:x>
        <pd:y>35</pd:y>
        <config>
            <processName>/Common/Sub Processes/Sub_Trace.process</processName>
        </config>
        <pd:inputBindings>
            <TraceIn>
                <MsgCode>
                    <xsl:value-of select="&quot;EAI-04055&quot;"/>
                </MsgCode>
                <MsgId>
                    <xsl:value-of select="eai:getMessageId($Receive-JMS-Request/ns2:ActivityOutput/Body)"/>
                </MsgId>
                <Parameter>
                    <xsl:value-of select="$Receive-JMS-Request/ns2:ActivityOutput/JMSHeaders/ns2:JMSDestination"/>
                </Parameter>
                <Parameter>
                    <xsl:value-of select="concat(tib:render-xml($Receive-JMS-Request/ns2:ActivityOutput/JMSHeaders, true(), true()),'&#xA;', tib:render-xml($Receive-JMS-Request/ns2:ActivityOutput/JMSProperties, true(), true()),'&#xA;', tib:render-xml($Receive-JMS-Request/ns2:ActivityOutput/OtherProperties, true(), true()))"/>
                </Parameter>
                <Parameter>
                    <xsl:value-of select="$Receive-JMS-Request/ns2:ActivityOutput/Body"/>
                </Parameter>
            </TraceIn>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>427</pd:x>
        <pd:y>282</pd:y>
        <config>
            <processName>/Common/Sub Processes/Sub_ErrorTrace.process</processName>
        </config>
        <pd:inputBindings>
            <ErrorTraceIn>
                <ns4:Errors>
                    <xsl:choose>
                        <xsl:when test="$_error_Process-Request">
                            <xsl:copy-of select="$_error_Process-Request/ns4:Error"/>
                        </xsl:when>
                        <xsl:when test="$_error_Parse-XML">
                            <ns4:Error>
                                <ns4:MsgCode>
                                    <xsl:value-of select="&quot;XAuth-03016&quot;"/>
                                </ns4:MsgCode>
                                <ns4:ActivityError>
                                    <xsl:value-of select="tib:render-xml($_error_Parse-XML/ActivityErrors, false(),true())"/>
                                </ns4:ActivityError>
                            </ns4:Error>
                        </xsl:when>
                        <xsl:otherwise>
                            <ns4:Error>
                                <ns4:MsgCode>
                                    <xsl:value-of select="&quot;EAI-03059&quot;"/>
                                </ns4:MsgCode>
                                <ns4:Params>
                                    <xsl:value-of select="&quot;ModifyMultiMediaLocationRegistration&quot;"/>
                                </ns4:Params>
                                <xsl:if test="$Receive-JMS-Request/ns2:ActivityOutput/Body">
                                    <ns4:MsgId>
                                        <xsl:value-of select="eai:getMessageId($Receive-JMS-Request/ns2:ActivityOutput/Body)"/>
                                    </ns4:MsgId>
                                </xsl:if>
                                <ns3:ErrorReport>
                                    <xsl:copy-of select="$_error/ns3:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                                    <xsl:copy-of select="$_error/ns3:ErrorReport/@*"/>
                                    <xsl:copy-of select="$_error/ns3:ErrorReport/node()"/>
                                </ns3:ErrorReport>
                                <ns4:ActivityError>
                                    <xsl:copy-of select="$_error/ns3:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                                    <xsl:copy-of select="$_error/ns3:ErrorReport/@*"/>
                                    <xsl:copy-of select="$_error/ns3:ErrorReport/node()"/>
                                </ns4:ActivityError>
                            </ns4:Error>
                        </xsl:otherwise>
                    </xsl:choose>
                    <ns4:Error>
                        <ns4:MsgCode>
                            <xsl:value-of select="&quot;EAI-03011&quot;"/>
                        </ns4:MsgCode>
                        <xsl:choose>
                            <xsl:when test="$Receive-JMS-Request/ns2:ActivityOutput/Body">
                                <ns4:Params>
                                    <xsl:value-of select="$Receive-JMS-Request/ns2:ActivityOutput/Body"/>
                                </ns4:Params>
                            </xsl:when>
                            <xsl:otherwise>
                                <ns4:Params>
                                    <xsl:value-of select="&quot;&quot;"/>
                                </ns4:Params>
                            </xsl:otherwise>
                        </xsl:choose>
                        <xsl:if test="$Receive-JMS-Request/ns2:ActivityOutput/Body">
                            <ns4:MsgId>
                                <xsl:value-of select="eai:getMessageId($Receive-JMS-Request/ns2:ActivityOutput/Body)"/>
                            </ns4:MsgId>
                        </xsl:if>
                    </ns4:Error>
                </ns4:Errors>
            </ErrorTraceIn>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>224</pd:x>
        <pd:y>129</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx5:SubmitOrder"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$Receive-JMS-Request/ns2:ActivityOutput/Body"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Call NotifyMultimediaRegistrationStatus">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>538</pd:x>
        <pd:y>127</pd:y>
        <config>
            <processName>/xAuthorityAdapter/Processes/Outbound/0337-NotifyMultimediaRegistrationStatus.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:choose>
                    <xsl:when test="exists($Process-Request)">
                        <ns10:getOrdeByKeyrResponse>
                            <ns10:orderValue>
                                <xsl:copy-of select="$Process-Request/output/OrderValue/ancestor-or-self::*/namespace::node()"/>
                                <xsl:copy-of select="$Process-Request/output/OrderValue/@*"/>
                                <xsl:copy-of select="$Process-Request/output/OrderValue/node()"/>
                            </ns10:orderValue>
                        </ns10:getOrdeByKeyrResponse>
                    </xsl:when>
                    <xsl:otherwise/>
                </xsl:choose>
                <xsl:choose>
                    <xsl:when test="exists($_error_Process-Request)">
                        <ErrorDescription>
                            <xsl:value-of select="if (exists($_error_Process-Request/ns4:Error/ns4:ApplicationError)) then&#xA;&#x9;$_error_Process-Request/ns4:Error/ns4:ApplicationError&#xA;else&#xA;&#x9;concat($_globalVariables/ns:GlobalVariables/xAuthorityAdapter/ErrorDescription/InternalError,&quot; ErrorReport - &quot;,tib:render-xml($_error_Process-Request/ns4:Error, false(), true()))"/>
                        </ErrorDescription>
                    </xsl:when>
                    <xsl:when test="count($Process-Request/output/OrderValue/ns10:failedServices/ns10:item)>=1">
                        <ErrorDescription>
                            <xsl:value-of select="$Process-Request/output/OrderValue/ns10:failedServices/ns10:item[1]/ns11:exception/ns11:message"/>
                        </ErrorDescription>
                    </xsl:when>
                    <xsl:when test="exists($_error_Parse-XML)">
                        <ErrorDescription>
                            <xsl:value-of select="tib:render-xml($_error_Parse-XML/ActivityErrors, false(), true())"/>
                        </ErrorDescription>
                    </xsl:when>
                    <xsl:when test="exists($_error)">
                        <ErrorDescription>
                            <xsl:value-of select="tib:render-xml($_error/ns3:ErrorReport, false(), true())"/>
                        </ErrorDescription>
                    </xsl:when>
                    <xsl:otherwise/>
                </xsl:choose>
                <xsl:if test="$Parse-XML/pfx5:SubmitOrder/ns1:CMH/ns1:FID">
                    <FID>
                        <xsl:value-of select="$Parse-XML/pfx5:SubmitOrder/ns1:CMH/ns1:FID"/>
                    </FID>
                </xsl:if>
                <OrderType>
                    <xsl:value-of select="$Parse-XML/pfx5:SubmitOrder/ns7:BODY/ORDER/ORDER_HEADER/ORDER_TYPE"/>
                </OrderType>
                <xsl:choose>
                    <xsl:when test="exists($_error_Process-Request) or count($Process-Request/output/OrderValue/ns10:failedServices/ns10:item)>=1 or exists($_error_Parse-XML) or exists($_error)">
                        <StatusCode>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/xAuthorityAdapter/StatusCode/Failed"/>
                        </StatusCode>
                    </xsl:when>
                    <xsl:otherwise>
                        <StatusCode>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/xAuthorityAdapter/StatusCode/Success"/>
                        </StatusCode>
                    </xsl:otherwise>
                </xsl:choose>
                <Product_Category>
                    <xsl:value-of select="$Parse-XML/pfx5:SubmitOrder/ns7:BODY/ORDER/ORDER_ITEM[1]/PRODUCT_CATEGORY"/>
                </Product_Category>
                <External-Reference>
                    <xsl:value-of select="$Parse-XML/pfx5:SubmitOrder/ns7:BODY/ORDER/ORDER_HEADER/EXTERNAL_REFERENCE"/>
                </External-Reference>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log Notify Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>631</pd:x>
        <pd:y>250</pd:y>
        <config>
            <processName>/Common/Sub Processes/Sub_ErrorTrace.process</processName>
        </config>
        <pd:inputBindings>
            <ErrorTraceIn>
                <ns4:Errors>
                    <xsl:copy-of select="$_error_Call-NotifyMultimediaRegistrationStatus/ns4:Error"/>
                </ns4:Errors>
            </ErrorTraceIn>
        </pd:inputBindings>
    </pd:activity>
