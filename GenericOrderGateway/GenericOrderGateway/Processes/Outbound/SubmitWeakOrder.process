                            <faultstring>
                                <xsl:value-of select="&quot;Validation Error&quot;"/>
                            </faultstring>
                            <detail>
                                <pfx7:WeakOrderFault>
                                    <pfx7:errorMessage>
                                        <xsl:value-of select="$Catch/pfx12:Error/pfx12:Params[1]"/>
                                    </pfx7:errorMessage>
                                    <pfx7:errorCode>
                                        <xsl:value-of select="&quot;500- Internal Server Error&quot;"/>
                                    </pfx7:errorCode>
                                    <xsl:for-each select="$_error_Validation-error/pfx12:Error/pfx12:Params">
                                        <pfx7:detail>
                                            <xsl:value-of select="."/>
                                        </pfx7:detail>
                                    </xsl:for-each>
                                </pfx7:WeakOrderFault>
                            </detail>
                        </FaultMessage>
                    </xsl:when>
                    <xsl:otherwise>
                        <FaultMessage>
                            <faultcode>
                                <xsl:value-of select="&quot;soap:server&quot;"/>
                            </faultcode>
                            <faultstring>
                                <xsl:value-of select="&quot;EAI Error&quot;"/>
