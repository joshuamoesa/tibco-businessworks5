            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Update Notification">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>305</pd:x>
            <pd:y>110</pd:y>
            <config>
                <timeout>%%Connections/DB/Oracle/MainStorage/ExecutionTimeOut%%</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <jdbcSharedConfig>/Shared Resources/Connections/DB/Oracle/MainStorage.sharedjdbc</jdbcSharedConfig>
                <statement>Update CFG_Notification
Set name = ?,
    description = ?, CHANNELID=?, MESSAGE = ?
where ID = ? and VersionID=?
</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>Name</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>Description</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>ChannelID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>Message</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>ID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>Version</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <Name>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Notification/pfx2:Name)">
                                <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:Name"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </Name>
                    <Description>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Notification/pfx2:Description)">
                                <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:Description"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </Description>
                    <ChannelID>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Notification/pfx2:ChannelID)">
                                <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:ChannelID"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </ChannelID>
                    <Message>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Notification/pfx2:Message)">
                                <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:Message"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </Message>
                    <ID>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Notification/pfx2:ID)">
                                <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:ID"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </ID>
                    <Version>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Notification/pfx2:Version)">
                                <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:Version"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </Version>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Delete Notification Parameters">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>409</pd:x>
            <pd:y>121</pd:y>
            <config>
                <timeout>%%Connections/DB/Oracle/MainStorage/ExecutionTimeOut%%</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <jdbcSharedConfig>/Shared Resources/Connections/DB/Oracle/MainStorage.sharedjdbc</jdbcSharedConfig>
                <statement>delete FROM  CFG_Notification_PROP where NotificationID=?</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>ID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <ID>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Notification/pfx2:ID)">
                                <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:ID"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </ID>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:group name="Loop over Parameters">
            <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
            <pd:resourceType>ae.process.group</pd:resourceType>
            <pd:x>611</pd:x>
            <pd:y>44</pd:y>
            <pd:width>167</pd:width>
            <pd:height>122</pd:height>
            <pd:collapsedWidth>57</pd:collapsedWidth>
            <pd:collapsedHeight>70</pd:collapsedHeight>
            <pd:groupVariables/>
            <pd:joinToggle>inherit</pd:joinToggle>
            <config>
                <pd:groupType>inputLoop</pd:groupType>
                <pd:serializable>false</pd:serializable>
                <pd:over>$RetrieveParameters/Output/Parameters</pd:over>
                <pd:iterationElementSlot>Parameter</pd:iterationElementSlot>
                <pd:indexSlot>i</pd:indexSlot>
                <pd:activityOutputName/>
                <pd:accumulateOutput>false</pd:accumulateOutput>
            </config>
            <pd:inputBindings/>
            <pd:expanded>true</pd:expanded>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>InsertParameter</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>InsertParameter</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:activity name="InsertParameter">
                <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
                <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
                <pd:x>683</pd:x>
                <pd:y>99</pd:y>
                <config>
                    <timeout>%%Connections/DB/Oracle/MainStorage/ExecutionTimeOut%%</timeout>
                    <commit>false</commit>
                    <emptyStrAsNil>false</emptyStrAsNil>
                    <statement>Insert into CFG_Notification_PROP (NotificationID,Name, Value)
VALUES (?,?,?)</statement>
                    <jdbcSharedConfig>/Shared Resources/Connections/DB/Oracle/MainStorage.sharedjdbc</jdbcSharedConfig>
                    <Prepared_Param_DataType>
                        <parameter>
                            <parameterName>ID</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                        <parameter>
                            <parameterName>Name</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                        <parameter>
                            <parameterName>Value</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                    </Prepared_Param_DataType>
                    <x__copyToClipboardReference>
                        <ref>
                            <pfx>xsd</pfx>
                            <type>N</type>
                            <ns>http://www.w3.org/2001/XMLSchema</ns>
                        </ref>
                    </x__copyToClipboardReference>
                </config>
                <pd:inputBindings>
                    <jdbcUpdateActivityInput>
                        <ID>
                            <xsl:choose>
                                <xsl:when test="exists($Start/Input/pfx2:Notification/pfx2:ID)">
                                    <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:ID"/>
                                </xsl:when>
                                <xsl:otherwise>
                                    <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                </xsl:otherwise>
                            </xsl:choose>
                        </ID>
                        <Name>
                            <xsl:value-of select="$Parameter/Parameters"/>
                        </Name>
                        <Value>
                            <xsl:choose>
                                <xsl:when test="exists($Start/Input/pfx2:Notification/pfx3:Parameters/pfx3:Parameter[pfx3:Name=$Parameter/Parameters]/pfx3:Value)">
                                    <xsl:value-of select="$Start/Input/pfx2:Notification/pfx3:Parameters/pfx3:Parameter[pfx3:Name=$Parameter/Parameters]/pfx3:Value"/>
                                </xsl:when>
                                <xsl:otherwise>
                                    <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                </xsl:otherwise>
                            </xsl:choose>
                        </Value>
                    </jdbcUpdateActivityInput>
                </pd:inputBindings>
            </pd:activity>
        </pd:group>
        <pd:activity name="RetrieveParameters">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>519</pd:x>
            <pd:y>102</pd:y>
            <config>
                <processName>/Processes/DataServices_Oracle/Configuration/Notification/RetrievePossibleParameters.process</processName>
            </config>
            <pd:inputBindings>
                <Input>
                    <xsl:if test="$Start/Input/pfx2:Notification/pfx2:ChannelID">
                        <ChannelID>
                            <xsl:value-of select="$Start/Input/pfx2:Notification/pfx2:ChannelID"/>
                        </ChannelID>
                    </xsl:if>
                </Input>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>ValidateOperation</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ValidateOperation</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
