    <pd:activity name="Map Weak to Strong">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>356</pd:x>
        <pd:y>190</pd:y>
        <config>
            <element>
                <xsd:element name="Request">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:any processContents="skip"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <Request>
                <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="product" select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderItem/pfx7:Product/pfx7:Name"/>
                <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="action" select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderItem/pfx7:Product/pfx7:Action/pfx7:Name"/>
                <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="nsPrefix" select="$_globalVariables/pfx6:GlobalVariables/GenericOrderGateway/Services/Outbound/SubmitWeakOrder/namespacePrefix"/>
                <xsl:variable name="version" select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderItem/pfx7:Product/pfx7:Version"/>
                <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="orderNamespace" select="$_globalVariables/pfx6:GlobalVariables/GenericOrderGateway/Services/Outbound/SubmitWeakOrder/orderNamespace"/>
                <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="groupNamespace" select="concat($nsPrefix,$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderItem/pfx7:Product/pfx7:Group)"/>
                <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="productNamespace" select="concat($groupNamespace,&quot;/&quot;,$product,&quot;/data/v&quot;, $version)"/>
                <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="request" select="concat($product, $action,&quot;Request&quot;)"/>
                <xsl:element namespace="{$productNamespace}" name="{$request}">
                    <xsl:element namespace="{$orderNamespace}" name="customerOrderId">
                        <xsl:value-of select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:customerOrderId"/>
                    </xsl:element>
                    <xsl:element namespace="{$orderNamespace}" name="orderDateTime">
                        <xsl:value-of select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:orderDateTime"/>
                    </xsl:element>
                    <xsl:if test="string-length($SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderParameters)!=0">
                        <xsl:element namespace="{$productNamespace}" name="OrderParameters">
                            <xsl:for-each select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderParameters/pfx7:Parameter">
                                <xsl:element namespace="{$groupNamespace}" name="{pfx7:Name}">
                                    <xsl:value-of select="pfx7:Value"/>
                                </xsl:element>
                            </xsl:for-each>
                        </xsl:element>
                    </xsl:if>
                    <xsl:if test="string-length($SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderItem)!=0">
                        <xsl:element namespace="{$productNamespace}" name="OrderItem">
                            <xsl:for-each select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderItem/pfx7:OrderItemParameters/pfx7:Parameter">
                                <xsl:element namespace="{$groupNamespace}" name="{pfx7:Name}">
                                    <xsl:value-of select="pfx7:Value"/>
                                </xsl:element>
                            </xsl:for-each>
                            <xsl:if test="string-length($SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderItem/pfx7:Product/pfx7:Action/pfx7:Parameters)!=0">
                                <xsl:element namespace="{$productNamespace}" name="Parameters">
                                    <xsl:for-each select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:OrderItem/pfx7:Product/pfx7:Action/pfx7:Parameters/pfx7:Parameter">
                                        <xsl:element namespace="{$productNamespace}" name="{pfx7:Name}">
                                            <xsl:value-of select="pfx7:Value"/>
                                        </xsl:element>
                                    </xsl:for-each>
                                </xsl:element>
                            </xsl:if>
                        </xsl:element>
                    </xsl:if>
                </xsl:element>
            </Request>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Publish SubmitOrder">
