											</complexType>
										</element>
										<element name="SUPPLIER_CONTACT_PERSON" minOccurs="0">
											<annotation>
												<documentation>Specifiy the service providing (e.g. KPN) organisation contact person detaills.</documentation>
												<documentation>Artificial sequence to separate.</documentation>
											</annotation>
											<complexType>
												<sequence>
													<element name="FAX_NUMBER" minOccurs="0">
														<annotation>
															<documentation>A fax number of the contact person can be specified in this field.</documentation>
															<documentation>Artificial sequence to separate.</documentation>
														</annotation>
														<complexType>
															<sequence>
																<element name="COUNTRY_CODE" type="xs:string" minOccurs="0">
																	<annotation>
																		<documentation>The country code that is to be used.</documentation>
																	</annotation>
																</element>
																<element name="AREA_CODE" type="xs:string">
																	<annotation>
																		<documentation>The area code of the subscriber</documentation>
																	</annotation>
																</element>
																<element name="SUBSCRIBER_NUMBER" type="xs:string">
																	<annotation>
																		<documentation>The telephone number of the subscription that can be used in the specified area code.</documentation>
																	</annotation>
																</element>
															</sequence>
														</complexType>
													</element>
													<element name="PHONE_NUMBER" minOccurs="0">
														<annotation>
															<documentation>The telephone number of the contact person that can be used to make contact.</documentation>
															<documentation>Artificial sequence to separate.</documentation>
														</annotation>
														<complexType>
															<sequence>
																<element name="COUNTRY_CODE" type="xs:string" minOccurs="0">
																	<annotation>
																		<documentation>The country code that is to be used.</documentation>
																	</annotation>
																</element>
																<element name="AREA_CODE" type="xs:string">
																	<annotation>
																		<documentation>The area code of the subscriber</documentation>
																	</annotation>
																</element>
																<element name="SUBSCRIBER_NUMBER" type="xs:string">
																	<annotation>
																		<documentation>The telephone number of the subscription that can be used in the specified area code.</documentation>
																	</annotation>
																</element>
															</sequence>
														</complexType>
													</element>
													<element name="GIVEN_NAME" type="tns:givenNames_type" minOccurs="0">
														<annotation>
															<documentation>First name of the contact person</documentation>
														</annotation>
													</element>
													<element name="INITIALS" type="tns:initials_type" minOccurs="0">
														<annotation>
															<documentation>Initials of the contact person</documentation>
														</annotation>
													</element>
													<element name="FAMILY_NAME" type="tns:familyName_type">
														<annotation>
															<documentation>Official surname of the contact person</documentation>
														</annotation>
													</element>
													<element name="FAMILY_NAME_PREFIX" type="tns:familyNamePrefix_type" minOccurs="0">
														<annotation>
															<documentation>A prefix for the surname. This can be something like, 'van', 'van der', etcetera.</documentation>
														</annotation>
													</element>
													<element name="FULL_NAME" type="xs:string" minOccurs="0">
														<annotation>
															<documentation>Full person name as single string.</documentation>
														</annotation>
													</element>
													<element name="PREFERRED_CONTACT_METHOD" type="xs:string" minOccurs="0">
														<annotation>
															<documentation>It can be specified in this field if a contact person has a preferred way to be contacted. 
Possible values are:
E-Mail
Fax
Telephone
															
															
															
															</documentation>
														</annotation>
													</element>
													<element name="EMAIL_ADDRESS" type="tns:emailAddress_type" minOccurs="0">
														<annotation>
															<documentation>The e-mail address of the contact person is supplied in this field</documentation>
														</annotation>
													</element>
												</sequence>
											</complexType>
										</element>
										<element name="PROGRESS_TYPE" type="xs:string" minOccurs="0">
											<annotation>
												<documentation>Specifies the type/kind of progress reported in the message.</documentation>
											</annotation>
										</element>
										<element name="INITIATOR" type="xs:string">
											<annotation>
												<documentation>Indicates which party initiated contact.</documentation>
											</annotation>
										</element>
										<element name="CONTACT_METHOD" type="xs:string">
											<annotation>
												<documentation>Indicates the mode of contact, e.i. mail, phone, </documentation>
											</annotation>
										</element>
										<element name="DESCRIPTION" type="xs:string" minOccurs="0">
											<annotation>
												<documentation>A short, to the point, description of the contact content, topics and arrangements</documentation>
											</annotation>
										</element>
										<element name="NEXT_EVENT_DATE" type="xs:date" minOccurs="0">
											<annotation>
												<documentation>Date for the next contact event. Any follow-up actions described should then be completed.</documentation>
											</annotation>
										</element>
										<element name="FOLLOWUP_ACTION_DESCRIPTION" type="xs:string" minOccurs="0">
											<annotation>
												<documentation>Describe actions, if any, to be undertaken before the next contact event.</documentation>
											</annotation>
										</element>
									</sequence>
								</complexType>
							</element>
							<element name="SERVICE_LEVEL_AGREEMENT" minOccurs="0">
								<annotation>
									<documentation>Information regarding the Service Level Agreement covering this interaction.</documentation>
									<documentation>Artificial sequence to separate.</documentation>
								</annotation>
								<complexType>
									<sequence>
										<element name="CATEGORY" type="xs:string">
											<annotation>
												<documentation>Specifies the Service Level Agreement category that covers this interaction.</documentation>
											</annotation>
										</element>
										<element name="SLA_DATE_COMPLETE" type="xs:dateTime" minOccurs="0">
											<annotation>
												<documentation>Specifies Service Level Agreement (SLA) based incident closure date and time; the time at which, according to SLA, the incident should be resolved.</documentation>
											</annotation>
										</element>
									</sequence>
								</complexType>
							</element>
							<element name="INTERACTION_STATUS" minOccurs="0">
								<annotation>
									<documentation>Interaction subject status information.</documentation>
									<documentation>Artificial sequence to separate.</documentation>
								</annotation>
								<complexType>
									<sequence>
										<element name="CUSTOMER_REQUEST_STATUS" type="xs:string" minOccurs="0">
											<annotation>
												<documentation>The Interaction customer request status specifies an optional status that can be assigned to the interaction.</documentation>
											</annotation>
										</element>
										<element name="INTERACTION_STATUS_DATE" type="xs:dateTime" minOccurs="0"/>
										<element name="CONFIRMATION_DUE_TIME" type="xs:integer" minOccurs="0"/>
										<element name="CUSTOMER_REQUEST_STATUS_ACCEPTED" type="xs:boolean" minOccurs="0"/>
										<element name="ERROR_CODE" type="xs:string" minOccurs="0">
											<annotation>
												<documentation>Error code detailing the status information.</documentation>
											</annotation>
										</element>
										<element name="ERROR_DESCRIPTION" type="xs:string" minOccurs="0">
											<annotation>
												<documentation>Status/error description, additional information.</documentation>
											</annotation>
										</element>
									</sequence>
								</complexType>
							</element>
							<element name="BUSINESS_INTERACTION_TYPE" type="tns:businessInteractionType_type">
								<annotation>
									<documentation>The Business Interaction Type specifies how to interpret this interaction. 
The current possible values are:
- INCIDENT_REPORT_REQUEST
- CHANGE_REQUEST
- REQUEST_FOR_INFORMATION
- REQUEST_FOR_QUOTATION
- CONFIRMATION
- STATUS_UPDATE
- STATUS_CONFIRMATION
- INTERACTION_UPDATE
									
									
									
									</documentation>
								</annotation>
							</element>
							<element name="BUSINESS_INTERACTION_SUB_TYPE" type="xs:string" minOccurs="0">
								<annotation>
									<documentation>This can be a sub type for the interaction. For Confirmations this can be Accept or Reject. For updates this field specifies the type of update etcetera.</documentation>
								</annotation>
							</element>
							<element name="INTERACTION_ID" type="xs:string" minOccurs="0">
								<annotation>
									<documentation>This field is used to specify the identification of the interaction as known by the service provider (e.g. KPN). This can be the Ticket ID, Change ID, etcetera. </documentation>
								</annotation>
							</element>
							<element name="EXTERNAL_ID" type="xs:string" minOccurs="0">
								<annotation>
									<documentation>This field contains the identifier that the customer has assigned to this interaction.</documentation>
								</annotation>
							</element>
							<element name="EXTERNAL_SUB_ID" type="xs:string" minOccurs="0">
								<annotation>
									<documentation>The customer can use this field to specify an additional ID for this interaction.</documentation>
								</annotation>
							</element>
							<element name="EXTERNAL_REFERENCE" type="xs:string" minOccurs="0">
								<annotation>
									<documentation>The external reference can be used to reference other tickets, incidents or other elements</documentation>
								</annotation>
							</element>
							<element name="INTERACTION_DATE" type="xs:dateTime" minOccurs="0">
								<annotation>
									<documentation>Date and time this interaction is to take place. This could be a wish date, planned date etc.</documentation>
								</annotation>
							</element>
							<element name="INTERACTION_DATE_COMPLETE" type="xs:dateTime" minOccurs="0">
								<annotation>
									<documentation>Date and time issues subject to this interaction are considered closed, not further actions or modifications will be allowed.</documentation>
								</annotation>
							</element>
							<element name="INTERACTION_REASON_CODE" type="xs:string" minOccurs="0">
								<annotation>
									<documentation>This field is used to inform about the reason of the interaction. For example, when a ticket is put on hold, this field can be used to give the reason for the hold action.</documentation>
								</annotation>
							</element>
							<element name="DESCRIPTION" type="tns:description_type" minOccurs="0">
								<annotation>
									<documentation>The description field contains a summary to indicate the intent of the interaction</documentation>
								</annotation>
							</element>
							<element name="IMPACT" type="xs:string" minOccurs="0">
								<annotation>
									<documentation>The impact field can be used by the customer to give their opinion on the impact on their business of the communicated change or incident.</documentation>
								</annotation>
							</element>
							<element name="URGENCY" type="xs:string" minOccurs="0">
								<annotation>
									<documentation>The customer can use the urgency field to indicate the urgency of this interaction.</documentation>
								</annotation>
							</element>
						</sequence>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
</schema>