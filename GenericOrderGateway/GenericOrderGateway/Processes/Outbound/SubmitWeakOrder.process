        </config>
        <pd:inputBindings>
            <xsl:variable name="order" select="$Map-Weak-to-Strong/Request/*"/>
            <xsl:variable name="orderItem" select="$order/*[local-name()=&quot;OrderItem&quot;]"/>
            <pfx11:SubmitOrder>
