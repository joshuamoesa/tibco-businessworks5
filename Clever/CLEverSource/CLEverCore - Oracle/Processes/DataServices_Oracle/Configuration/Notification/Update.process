                    <xsd:element ref="pfx3:Security"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>26</pd:startX>
    <pd:startY>99</pd:startY>
    <pd:returnBindings>
        <pfx3:ID>
            <xsl:value-of select="$GetSequence/resultSet/Record[1]/NEXTVAL"/>
        </pfx3:ID>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endX>889</pd:endX>
    <pd:endY>102</pd:endY>
    <pd:errorSchemas/>
