                            </pfx2:Parameter>
                        </xsl:for-each>
                        <xsl:if test="$Resolution/Record/VERSIONID">
                            <pfx3:Version>
                                <xsl:value-of select="$Resolution/Record/VERSIONID"/>
                            </pfx3:Version>
                        </xsl:if>
                    </pfx2:Resolution>
                </pd:inputBindings>
            </pd:activity>
