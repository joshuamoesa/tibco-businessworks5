        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>708</pd:x>
        <pd:y>375</pd:y>
        <config>
            <faultName>Exception</faultName>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <data>
                    <xsl:choose>
                        <xsl:when test="exists($_error_Publish-SubmitOrder)">
                            <xsl:copy-of select="$_error_Publish-SubmitOrder/pfx12:Error"/>
                        </xsl:when>
                        <xsl:when test="exists($_error_SOAPSendReply)">
                            <pfx12:Error>
                                <pfx12:MsgCode>
                                    <xsl:value-of select="&quot;GOG-03002&quot;"/>
                                </pfx12:MsgCode>
                                <pfx12:Params>
                                    <xsl:value-of select="$_error_SOAPSendReply/ns5:ServerException/msg"/>
                                </pfx12:Params>
                                <pfx12:MsgId>
                                    <xsl:value-of select="$ID/messageId/GUID"/>
                                </pfx12:MsgId>
                                <xsl:copy-of select="$_error/ns4:ErrorReport"/>
                            </pfx12:Error>
                        </xsl:when>
                        <xsl:otherwise>
                            <pfx12:Error>
                                <pfx12:MsgCode>
                                    <xsl:value-of select="&quot;GOG-03202&quot;"/>
                                </pfx12:MsgCode>
                                <pfx12:Params>
                                    <xsl:value-of select="&quot;Mapping Error in SubmitWeakOrder&quot;"/>
                                </pfx12:Params>
                                <pfx12:MsgId>
                                    <xsl:value-of select="$ID/messageId/GUID"/>
                                </pfx12:MsgId>
