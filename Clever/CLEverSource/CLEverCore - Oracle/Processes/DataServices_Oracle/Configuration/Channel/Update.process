<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:pfx="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns2="http://www.tibco.com/CLEver/namespace/Custom" xmlns:pfx2="http://www.tibco.com/CLEver/Channel" xmlns:pfx3="http://www.tibco.com/CLEver/ExtraTypes" xmlns:ns3="http://www.tibco.com/CLEver/Exception" xmlns:ns4="http://www.tibco.com/CLEver/InternalException">
    <xsd:import namespace="http://www.tibco.com/CLEver/Channel" schemaLocation="/Shared Resources/Schema Definitions/CLEver/Channel.xsd"/>
    <xsd:import namespace="http://www.tibco.com/CLEver/ExtraTypes" schemaLocation="/Shared Resources/Schema Definitions/CLEver/ExtraTypes.xsd"/>
    <pd:name>Processes/DataServices_Oracle/Configuration/Channel/Update.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element ref="pfx2:Channel"/>
                    <xsd:element ref="pfx3:Security"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>26</pd:startX>
    <pd:startY>99</pd:startY>
    <pd:returnBindings>
        <pfx3:ID>
            <xsl:value-of select="$GetSequence/resultSet/Record[1]/NEXTVAL"/>
        </pfx3:ID>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endX>889</pd:endX>
    <pd:endY>102</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:activity name="ValidateOperation">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>140</pd:x>
        <pd:y>98</pd:y>
        <config>
            <processName>/Processes/DataServices/Security/SecurityEntryPoint/ValidateOperation.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:ValidateOperation>
                <xsl:copy-of select="$Start/Input/pfx3:Security"/>
                <pfx3:Object>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/StaticVariables/ConfigurationObjects/Channel/Name"/>
                </pfx3:Object>
                <pfx3:Operation>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/StaticVariables/Operations/Update"/>
                </pfx3:Operation>
                <xsl:if test="$Start/Input/pfx2:Channel/pfx3:Version">
                    <pfx3:Version>
                        <xsl:value-of select="$Start/Input/pfx2:Channel/pfx3:Version"/>
                    </pfx3:Version>
                </xsl:if>
            </pfx3:ValidateOperation>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Transactional group">
        <pd:type>com.tibco.pe.core.TransactionGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>199</pd:x>
        <pd:y>4</pd:y>
        <pd:width>643</pd:width>
        <pd:height>193</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>transactionGroup</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:transactionType>JDBCTransaction</pd:transactionType>
            <pd:includeCheckpoint>false</pd:includeCheckpoint>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Update Channel</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Loop over Parameters</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Update Channel</pd:from>
            <pd:to>Delete Channel Parameters</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Delete Channel Parameters</pd:from>
            <pd:to>RetrieveMechanism</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>RetrieveMechanism</pd:from>
            <pd:to>Loop over Parameters</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>RetrieveMechanism</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:xpathDescription>Not Found</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>count($RetrieveMechanism/Output/Mechanism[Name=$Start/Input/pfx2:Channel/pfx2:TransportMechanism])&lt;1</pd:xpath>
        </pd:transition>
        <pd:activity name="Update Channel">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>288</pd:x>
            <pd:y>110</pd:y>
            <config>
                <timeout>%%Connections/DB/Oracle/MainStorage/ExecutionTimeOut%%</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <jdbcSharedConfig>/Shared Resources/Connections/DB/Oracle/MainStorage.sharedjdbc</jdbcSharedConfig>
                <statement>Update CFG_Channel
Set name = ?,
    description = ?,
    TRANSPORTMECHANISM = ?
where ID = ? and VersionID=?
</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>Name</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>Description</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>TRANSPORTMECHANISM</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>ID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>Version</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <Name>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Channel/pfx2:Name)">
                                <xsl:value-of select="$Start/Input/pfx2:Channel/pfx2:Name"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </Name>
                    <Description>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Channel/pfx2:Description)">
                                <xsl:value-of select="$Start/Input/pfx2:Channel/pfx2:Description"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </Description>
                    <TRANSPORTMECHANISM>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Channel/pfx2:TransportMechanism)">
                                <xsl:value-of select="$Start/Input/pfx2:Channel/pfx2:TransportMechanism"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </TRANSPORTMECHANISM>
                    <ID>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Channel/pfx2:ID)">
                                <xsl:value-of select="$Start/Input/pfx2:Channel/pfx2:ID"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </ID>
                    <Version>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Channel/pfx3:Version)">
                                <xsl:value-of select="$Start/Input/pfx2:Channel/pfx3:Version"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </Version>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:group name="Loop over Parameters">
            <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
            <pd:resourceType>ae.process.group</pd:resourceType>
            <pd:x>634</pd:x>
            <pd:y>41</pd:y>
            <pd:width>167</pd:width>
            <pd:height>122</pd:height>
            <pd:collapsedWidth>57</pd:collapsedWidth>
            <pd:collapsedHeight>70</pd:collapsedHeight>
            <pd:groupVariables/>
            <pd:joinToggle>inherit</pd:joinToggle>
            <config>
                <pd:groupType>inputLoop</pd:groupType>
                <pd:serializable>false</pd:serializable>
                <pd:over>$RetrieveMechanism/Output/Mechanism[Name=$Start/Input/pfx2:Channel/pfx2:TransportMechanism]/Parameters</pd:over>
                <pd:iterationElementSlot>Mechanism</pd:iterationElementSlot>
                <pd:indexSlot>i</pd:indexSlot>
                <pd:activityOutputName/>
                <pd:accumulateOutput>false</pd:accumulateOutput>
            </config>
            <pd:inputBindings/>
            <pd:expanded>true</pd:expanded>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>Insert Parameters</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>Insert Parameters</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:activity name="Insert Parameters">
                <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
                <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
                <pd:x>709</pd:x>
                <pd:y>107</pd:y>
                <config>
                    <timeout>%%Connections/DB/Oracle/MainStorage/ExecutionTimeOut%%</timeout>
                    <commit>false</commit>
                    <emptyStrAsNil>false</emptyStrAsNil>
                    <statement>Insert into CFG_Channel_PROP (ChannelID,Name, Value)
VALUES (?,?,?)</statement>
                    <jdbcSharedConfig>/Shared Resources/Connections/DB/Oracle/MainStorage.sharedjdbc</jdbcSharedConfig>
                    <Prepared_Param_DataType>
                        <parameter>
                            <parameterName>ID</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                        <parameter>
                            <parameterName>Name</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                        <parameter>
                            <parameterName>Value</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                    </Prepared_Param_DataType>
                </config>
                <pd:inputBindings>
                    <jdbcUpdateActivityInput>
                        <ID>
                            <xsl:choose>
                                <xsl:when test="exists($Start/Input/pfx2:Channel/pfx2:ID)">
                                    <xsl:value-of select="$Start/Input/pfx2:Channel/pfx2:ID"/>
                                </xsl:when>
                                <xsl:otherwise>
                                    <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                </xsl:otherwise>
                            </xsl:choose>
                        </ID>
                        <Name>
                            <xsl:value-of select="$Mechanism/Parameters"/>
                        </Name>
                        <Value>
                            <xsl:choose>
                                <xsl:when test="exists($Start/Input/pfx2:Channel/pfx2:Parameter[pfx2:Name=$Mechanism/Parameters]/pfx2:Value)">
                                    <xsl:value-of select="$Start/Input/pfx2:Channel/pfx2:Parameter[pfx2:Name=$Mechanism/Parameters]/pfx2:Value"/>
                                </xsl:when>
                                <xsl:otherwise>
                                    <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                </xsl:otherwise>
                            </xsl:choose>
                        </Value>
                    </jdbcUpdateActivityInput>
                </pd:inputBindings>
            </pd:activity>
        </pd:group>
        <pd:activity name="Delete Channel Parameters">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>409</pd:x>
            <pd:y>121</pd:y>
            <config>
                <timeout>%%Connections/DB/Oracle/MainStorage/ExecutionTimeOut%%</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <jdbcSharedConfig>/Shared Resources/Connections/DB/Oracle/MainStorage.sharedjdbc</jdbcSharedConfig>
                <statement>delete FROM  CFG_Channel_PROP where ChannelID=?</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>ID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <ID>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Input/pfx2:Channel/pfx2:ID)">
                                <xsl:value-of select="$Start/Input/pfx2:Channel/pfx2:ID"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </ID>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="RetrieveMechanism">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>537</pd:x>
            <pd:y>68</pd:y>
            <config>
                <processName>/Processes/DataServices_Oracle/Configuration/Channel/RetrieveMechanismChannelParams.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Start/Input/pfx3:Security"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>537</pd:x>
            <pd:y>139</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <pfx:ActivityInput>
                    <message>
                        <xsl:value-of select="concat('Unknown Mechanism ',$Start/Input/pfx2:Channel/pfx2:TransportMechanism)"/>
                    </message>
                </pfx:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>ValidateOperation</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ValidateOperation</pd:from>
        <pd:to>Transactional group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Transactional group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>