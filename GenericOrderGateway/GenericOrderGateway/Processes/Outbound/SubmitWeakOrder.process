                                        </xsl:for-each>
                                    </CUSTOM_ITEM_PARAMETERS>
                                    <ACTION_PARAMETERS>
                                        <xsl:for-each select="$orderItem/*[local-name()=&quot;Parameters&quot;]/*">
                                            <xsl:copy-of select="." copy-namespaces="no"/>
                                        </xsl:for-each>
                                    </ACTION_PARAMETERS>
                                </GENERIC_ORDER_PRODUCT>
                            </PRODUCT>
                        </ORDER_ITEM>
                    </ORDER>
                </ns23:BODY>
