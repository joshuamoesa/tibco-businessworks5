                    <ns22:ID>
                        <xsl:value-of select="'435'"/>
                    </ns22:ID>
                    <ns22:NAME>
                        <xsl:value-of select="'SubmitOrder'"/>
                    </ns22:NAME>
                    <xsl:if test="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:customerOrderId">
                        <ns22:FID>
                            <xsl:value-of select="$SOAPEventSource/inputMessage/pfx7:WeakOrder/pfx7:customerOrderId"/>
                        </ns22:FID>
                    </xsl:if>
                    <ns22:MID>
                        <xsl:value-of select="'1080.10'"/>
                    </ns22:MID>
                    <ns22:PARADIGM>
                        <xsl:value-of select="'PUBLISH'"/>
                    </ns22:PARADIGM>
                    <ns22:OBJECT>
                        <xsl:value-of select="'Order'"/>
                    </ns22:OBJECT>
                    <ns22:VERSION>
                        <xsl:value-of select="'1.0'"/>
                    </ns22:VERSION>
