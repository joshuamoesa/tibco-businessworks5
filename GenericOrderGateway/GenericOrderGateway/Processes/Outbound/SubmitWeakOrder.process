                        </xsl:attribute>
                        <xsl:attribute name="ns22:COMPONENT_VERSION">
                            <xsl:value-of select="$_globalVariables/pfx6:GlobalVariables/GenericOrderGateway/ComponentVersion"/>
                        </xsl:attribute>
                    </ns22:FROM>
                    <ns22:TO>
                        <xsl:attribute name="ns22:DESTINATION">
                            <xsl:value-of select="concat($_globalVariables/pfx6:GlobalVariables/Env, '.FIXED.', $_globalVariables/pfx6:GlobalVariables/GenericOrderGateway/ComponentDomain, '.', $_globalVariables/pfx6:GlobalVariables/GenericOrderGateway/ComponentId, '.Publish.SubmitOrder.1')"/>
                        </xsl:attribute>
                    </ns22:TO>
                    <ns22:TRACKING>
                        <ns22:MESSAGE_ID>
                            <xsl:value-of select="$ID/messageId/GUID"/>
                        </ns22:MESSAGE_ID>
                        <ns22:ITEM>
                            <xsl:attribute name="ns22:APPINFO">
                                <xsl:value-of select="concat($_processContext/ns4:ProcessContext/ProjectName, '.', $_processContext/ns4:ProcessContext/EngineName)"/>
                            </xsl:attribute>
                            <xsl:attribute name="ns22:EVENT">
                                <xsl:value-of select="'PUBLISH'"/>
                            </xsl:attribute>
                            <xsl:attribute name="ns22:TIMESTAMP">
                                <xsl:value-of select="tib:timestamp()"/>
                            </xsl:attribute>
                        </ns22:ITEM>
                    </ns22:TRACKING>
                </ns22:CMH>
                <ns23:BODY>
