	<xs:simpleType name="cityName_type">
		<xs:annotation>
			<xs:documentation>The name of the city as it occurs in the property address of the customer organization.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CKRNumber_type">
		<xs:annotation>
			<xs:documentation>CKR number for the customer.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="countryName_type">
		<xs:annotation>
			<xs:documentation>The name of the country in which the customer organization is settled.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="houseNumber_type">
		<xs:annotation>
			<xs:documentation>The housenumber as it occurs in the property address of the customer organization.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="houseNumberExtension_type">
		<xs:annotation>
			<xs:documentation>The housenumber extension as it occurs in the property address of the customer organization.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="6"/>
			<xs:minLength value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="streetName_type">
		<xs:annotation>
			<xs:documentation>The name of the street as it occurs in the property address of the customer organization.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="24"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="genericZipcode_type">
		<xs:annotation>
			<xs:documentation>The zipcode as it occurs in the property address of the customer organization.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="dutchPostcodeNumbers_type">
		<xs:annotation>
			<xs:documentation>Specifies the customer address dutch postcode number part.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="dutchPostcodeLetters_type">
		<xs:annotation>
			<xs:documentation>Specifies the customer address dutch postcode letters part.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="givenNames_type">
		<xs:annotation>
			<xs:documentation>First name of the contact person</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="initials_type">
		<xs:annotation>
			<xs:documentation>Initials of the contact person</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="familyName_type">
		<xs:annotation>
			<xs:documentation>Official surname of the contact person</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="70"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="familyNamePrefix_type">
		<xs:annotation>
			<xs:documentation>A prefix for the surname. This can be something like, 'van', 'van der', etcetera.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="emailAddress_type">
		<xs:annotation>
			<xs:documentation>The e-mail address of the contact person is supplied in this field</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="320">
				<xs:annotation>
					<xs:documentation>Maximum length of an email address according to RFC 2821 is 64 for the local part (mail box) and 255 for the global part (domain). With the extra space for the '@' character the total is 320.</xs:documentation>
				</xs:annotation>
			</xs:maxLength>
			<xs:pattern value="([\.a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(([a-zA-Z0-9_-])*\.([a-zA-Z0-9_-])+)+">
				<xs:annotation>
					<xs:documentation>Validates whether an string conforms to the RFC 2822 which describes (among others) e-mail addresses.</xs:documentation>
				</xs:annotation>
			</xs:pattern>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumberCountryCode_type">
		<xs:annotation>
			<xs:documentation>The country code of the phone number.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
			<xs:minLength value="1"/>
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumberAreaCode_type">
		<xs:annotation>
			<xs:documentation>The area code of the phone number.
	  The area code specifies a national destination code: identifying a numbering area
	  within a numbering plan for a country (or countries sharing such numbering plan).
	  Such a numbering plan may be absent, may represent a geographical, logical, network
	  or other basis or a combination thereof for a numbering scheme.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"/>
			<xs:minLength value="1"/>
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="\d*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumberSubscriberNumber_type">
		<xs:annotation>
			<xs:documentation>The subscriber number of the phone number in the specified area code.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="15"/>
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumberFreeFormat_type">
		<xs:annotation>
			<xs:documentation>An unstructured representation of a phoneNumber.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="25"/>
			<xs:minLength value="1"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumberFormatted_type">
		<xs:annotation>
			<xs:documentation>A string representing a phoneNumber containing only digits and optionally white space,
		  possibly preceded by a plus sign.
		  A formatted phone number is dialable when the + symbol is replaced by
		  the digits required to access an international phone line and where all other
		  characters other than digits are ignored.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="tns:phoneNumberFreeFormat_type">
			<xs:pattern value="\+?[\d\-\s]{1,25}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumber_type">
		<xs:annotation>
			<xs:documentation>A string representing a phoneNumber containing digits and
		  possibly white space, hyphen, parenthesis pairs, and a preceding a plus sign.
		  An unformatted phone number requires human interpretation
		  (e.g. leave out leading 0 from area code when using country code)
		  to infer the actual phone number to be dialed.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="tns:phoneNumberFreeFormat_type">
			<xs:pattern value="\+?[\d\-\(\)\s]{1,25}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="incidentReportId_type">
		<xs:annotation>
			<xs:documentation>A unique identifier for the interaction report. This field is used to specify the identification of the interaction subject as known by the service provider (e.g. KPN). This can be the Ticket ID, Change ID, etcetera. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="structuredPhoneNumber_type">
		<xs:choice>
			<xs:element name="DETAILED">
				<xs:annotation>
					<xs:documentation>A structured representation of a phone number,
		  separating country code, area code and subscriber number.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="COUNTRY_CODE" type="tns:phoneNumberCountryCode_type" minOccurs="0"/>
						<xs:element name="AREA_CODE" type="tns:phoneNumberAreaCode_type" minOccurs="0"/>
						<xs:element name="SUBSCRIBER_NUMBER" type="tns:phoneNumberSubscriberNumber_type"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="FORMATTED" type="tns:phoneNumberFormatted_type">
				<xs:annotation>
					<xs:documentation>A string representing a phoneNumber containing only digits and optionally white space,
		  possibly preceded by a plus sign.
		  A formatted phone number is dialable when the + symbol is replaced by
		  the digits required to access an international phone line and where all other
		  characters other than digits are ignored.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UNFORMATTED" type="tns:phoneNumber_type">
				<xs:annotation>
					<xs:documentation>A string representing a phoneNumber containing digits and
		  possibly white space, hyphen, parenthesis pairs, and a preceding a plus sign.
		  An unformatted phone number requires human interpretation
		  (e.g. leave out leading 0 from area code when using country code)
		  to infer the actual phone number to be dialed.
		  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FREEFORMAT" type="tns:phoneNumberFreeFormat_type">
				<xs:annotation>
					<xs:documentation>A string that can not be dialed but can by used by a human to determine a phone number.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
