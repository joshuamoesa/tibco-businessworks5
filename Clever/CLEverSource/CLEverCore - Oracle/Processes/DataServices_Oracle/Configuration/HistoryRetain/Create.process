                    <Prepared_Param_DataType>
                        <parameter>
                            <parameterName>ID</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                        <parameter>
                            <parameterName>Name</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                        <parameter>
                            <parameterName>Value</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                    </Prepared_Param_DataType>
                </config>
                <pd:inputBindings>
                    <jdbcUpdateActivityInput>
                        <xsl:variable name="PropName" select="tib:tokenize($_globalVariables/ns1:GlobalVariables/StaticVariables/ConfigurationObjects/HistoryRetain/Parameters, &quot;,&quot;)[$i]"/>
