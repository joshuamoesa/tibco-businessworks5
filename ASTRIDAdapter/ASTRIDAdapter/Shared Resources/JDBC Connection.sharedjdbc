<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>JDBC Connection</name>
    <resourceType>ae.shared.JDBCSharedResource</resourceType>
    <config>
        <driver>tibcosoftwareinc.jdbc.oracle.OracleDriver</driver>
        <maxConnections>%%Common/JDBC/MaxConnections%%</maxConnections>
        <loginTimeout>%%Common/JDBC/DatabaseTimeOut%%</loginTimeout>
        <connectionType>JDBC</connectionType>
        <UseSharedJndiConfig>false</UseSharedJndiConfig>
        <location>jdbc:tibcosoftwareinc:oracle://%%Common/JDBC/Host%%:%%Common/JDBC/Port%%;SID=%%Common/JDBC/SID%%</location>
        <password>%%ASTRIDAdapter/JDBC/Password%%</password>
        <user>%%ASTRIDAdapter/JDBC/UserName%%</user>
    </config>
</BWSharedResource>