                    <element name="UpdatedOrder" type="tns:UpdatedSNIOrderType"/>
                </sequence>
            </complexType>
            <complexType name="HeaderType">
                <all>
                    <element name="CorrelationId" type="string">
                        <annotation>
                            <documentation>Field which can be used by Service Provider to correlate request and response. WOD do not fill or change this field.</documentation>
                        </annotation>
                    </element>
                    <element name="RequestId" type="string">
                        <annotation>
                            <documentation>Needs to be defined by KPN.</documentation>
                        </annotation>
                    </element>
                    <element name="Synchronous" type="boolean">
                        <annotation>
                            <documentation>Needs to be defined by KPN.</documentation>
                        </annotation>
                    </element>
                    <element name="Originator" type="string">
                        <annotation>
                            <documentation>Party sending the EAI message - needs more explanation.</documentation>
                        </annotation>
                    </element>
                    <element name="Recipient" type="string">
                        <annotation>
                            <documentation>Party intended to receive the EAI message.</documentation>
                        </annotation>
                    </element>
                </all>
            </complexType>
            <complexType name="UpdatedSNIOrderType">
                <all>
                    <element maxOccurs="1" minOccurs="0" name="OrderId" type="string">
                        <annotation>
                            <documentation>                                Unique identifier of order generated by
                                SNI.
                            
							</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="ProductInstanceId" type="string">
                        <annotation>
                            <documentation>                                Unique product instance id which was created by SNI.
                            
							</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="OrderStatus" type="tns:OrderStatusType">
                        <annotation>
                            <documentation>                                Status of order in SNI.
                            
							</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="Errors" type="tns:ErrorsType">
                        <annotation>
                            <documentation>                                Errors if any occurs.s
                            
							</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="Parameters" type="tns:ParametersType">
                        <annotation>
                            <documentation>                                Parameters, which may be related with
                                current state of order. E.g. date when
                                product will be available for client.
                            
							</documentation>
                        </annotation>
                    </element>
                </all>
            </complexType>
            <simpleType name="OrderStatusType">
                <restriction base="string">
                    <enumeration value="RECEIVED"/>
                    <enumeration value="ACCEPTED"/>
                    <enumeration value="PLANNED"/>
                    <enumeration value="REJECTED"/>
                    <enumeration value="COMPLETED"/>
                    <enumeration value="CANCELLED"/>
                    <enumeration value="FAULT"/>
                    <enumeration value="UNKNOWN"/>
                </restriction>
            </simpleType>
            <complexType name="ParameterType">
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="Name" type="string"/>
                    <element maxOccurs="1" minOccurs="1" name="Value" type="string"/>
                </sequence>
            </complexType>
            <complexType name="ParametersType">
                <sequence>
                    <element maxOccurs="unbounded" minOccurs="0" name="Parameter" type="tns:ParameterType"/>
                </sequence>
            </complexType>
            <complexType name="ErrorsType">
                <sequence>
                    <element maxOccurs="unbounded" minOccurs="0" name="Error" type="tns:ErrorType"/>
                </sequence>
            </complexType>
            <complexType name="ErrorType">
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="ErrorCode" type="string">
                        <annotation>
                            <documentation>Error code.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="ErrorDescription" type="string">
                        <annotation>
                            <documentation>                                Description for error.
                            
							</documentation>
