                                    <xsl:value-of select="&quot;EAI-03057&quot;"/>
                                </ns4:MsgCode>
                                <ns4:Params>
                                    <xsl:value-of select="&quot;ReleaseNumbers&quot;"/>
                                </ns4:Params>
                                <xsl:if test="$Receive-JMS-Publication/ns2:ActivityOutput/Body">
