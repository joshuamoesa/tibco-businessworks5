													<xs:element name="ATTRIBUTE" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>The attributes of the interaction item that are affected by this interaction.

(Note: corresponds to MOR1 - managed object report type 1).</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="NAME" type="xs:string">
																	<xs:annotation>
																		<xs:documentation>Name of the attribute</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="OLD_VALUE" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Old value of the attribute.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="NEW_VALUE" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The new value of the attribute. </xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="PROPERTY_ADDRESS" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Location of the object or service delivery.</xs:documentation>
														</xs:annotation>
														<xs:complexType mixed="false">
															<xs:complexContent mixed="false">
																<xs:extension base="tns:address_type">
																	<xs:sequence>
																		<xs:element name="LOCATION_CODE" type="xs:string" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Location code of the location of the managed object</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="INTERACTION_ITEM_ID" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The unique ID of the of the item that is specified in a mutually agreed item repository.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="INTERACTION_ITEM_NAME" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The unique name of the of the item that is specified in a mutually agreed item repository.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ACTION" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>NEW, CHANGED or DELETED.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CLASS" type="xs:string">
														<xs:annotation>
															<xs:documentation>Type of interaction item that is to be created, changed or deleted.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="SERVICE_NAME" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Contract identifier to which this customer interaction relates.
This can be a service or product name from a mutually agreed to service portfolio.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="PROVIDER_ID" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Identification of the provider that supports / delivers the service.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="PROVIDER_NAME" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Name of the provider that supports / delivers the service.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="MARKET_SEGMENT" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Identification of market segmentation for service.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="INTERACTION_STATUS" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Interaction subject status information.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="CUSTOMER_REQUEST_STATUS" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The Customer Request Status specifies an optional status that can be assigned to the interaction.
						For states related to incidents, this denotes the current state of the incident report, for which X.790 defines values.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_STATUS_DATE" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Date and time of status change.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CONFIRMATION_DUE_TIME" type="xs:integer" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The period, expressed in number of regular working hours, in which the customer is expected to confirm of reject the changes described in the Customer Service Request.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CUSTOMER_REQUEST_STATUS_ACCEPTED" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This field is meaningful in a state change confirmation message, where it indicates that the sending party accepts or rejects a state change as requested by the other party in an earlier state change request message.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ERROR_CODE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Error code detailing the status information.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ERROR_DESCRIPTION" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Status/error description, additional information.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INCIDENT_REPORT_STATE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Denotes the current state of the incident report. These are defined in X.790</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INCIDENT_REPORT_DATE" type="xs:dateTime" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="PROGRESS_EVENT" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Provides information concerning the reported progress (status change).</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="CUSTOMER_CONTACT_PERSON" type="tns:contactPerson_type" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Specifiy the service using organisation contact person detaills.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SUPPLIER_CONTACT_PERSON" type="tns:contactPerson_type" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Specifiy the service providing (e.g. KPN) organisation contact person detaills.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ACTIVITY_INFORMATION_LIST" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ACTICITY_INFORMATION" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="ACTIVITY_INFORMATION_VALUE" type="xs:string" minOccurs="0"/>
																<xs:element name="ACTIVITY_INFORMATION_CHANGE_DATE" type="xs:dateTime"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="QUESTION_LIST" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="QUESTION" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="ANALYSIS_QUESTION" type="xs:string"/>
																<xs:element name="ANALYSIS_ANSWER" type="xs:string" minOccurs="0"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="PROGRESS_TYPE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Specifies the type/kind of progress reported in the message.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INITIATOR" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Indicates which party initiated contact.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CONTACT_METHOD" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Indicates the mode of contact, e.i. mail, phone, </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DESCRIPTION" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>A short, to the point, description of the contact content, topics and arrangements</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="NEXT_EVENT_DATE" type="xs:date" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Date for the next contact event. Any follow-up actions described should then be completed.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="FOLLOWUP_ACTION_DESCRIPTION" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Describe actions, if any, to be undertaken before the next contact event.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ANALYSIS_INFORMATION" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Current information on the analysis of the trouble.
</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CLOSURE_SERVICE_TYPE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>A predefined catagory indicating in general the solution for the trouble.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="SERVICE_LEVEL_AGREEMENT" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Information regarding the Service Level Agreement covering this interaction.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="CATEGORY" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Specifies the Service Level Agreement category that covers this interaction.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SLA_FUNCTION" type="xs:string" minOccurs="0"/>
										<xs:element name="SLA_NAME" type="xs:string" minOccurs="0"/>
										<xs:element name="SLA_CLEARING_DATE" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Specifies Service Level Agreement (SLA) based incident closure date and time; the time at which, according to SLA, the incident should be resolved. (Clearing time indicate the 'deadline' after which the SLA is violated.)</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="CUSTOMER_ORGANIZATION" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The customer represents the external party which whom KPN interacts.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="PROPERTY_ADDRESS" type="tns:address_type" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Specifies the relevant customer address.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CONTACT_PERSON" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>The contact persons for this interaction are specified in these fields. Possible contact persons are the reporting person and the handling contact person. This is indicated in the CONTACT_ROLE.</xs:documentation>
											</xs:annotation>
											<xs:complexType mixed="false">
												<xs:complexContent mixed="false">
													<xs:extension base="tns:contactPerson_type">
														<xs:sequence>
															<xs:element name="CONTACT_ROLE" type="xs:string">
																<xs:annotation>
																	<xs:documentation>The role this contact person has in the interaction.
Allowed values are:
-Contact
-Reporter</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="CUSTOMER_ORGANIZATION_NAME" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The name of the organization of the customer to who is affected by the trouble or incident</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CUSTOMER_UNIT_NAME" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This field can be used to specify the customer business unit that is involved in this report.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SUPPLIER_CONTACT_PERSON_NAME" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The service provider responsible manager.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="CUSTOMER_ID" type="tns:customerID_type" minOccurs="0">
											<xs:annotation>
												<xs:documentation>KPN's identification number for the customer.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="CUSTOMER_INTERACTION_REPORT">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="B2B_IDENTIFICATION" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Customer request identification information.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="CUSTOMER_INTERACTION_SUPPLIER_REFERENCE_ID" type="tns:incidentReportId_type" minOccurs="0">
														<xs:annotation>
															<xs:documentation>A unique identifier for the interaction report. This field is used to specify the identification of the interaction subject as known by the service provider (e.g. KPN). This can be the Ticket ID, Change ID, etcetera. </xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CUSTOMER_INTERACTION_CONSUMER_ID" type="tns:incidentReportId_type" minOccurs="0">
														<xs:annotation>
															<xs:documentation>This field contains the identifier that the customer has assigned to this interaction.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CUSTOMER_INTERACTION_CONSUMER_SUB_ID" type="tns:incidentReportId_type" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The customer can use this field to specify an additional ID for this interaction.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="EAI_IDENTIFICATION" minOccurs="0">
											<xs:annotation>
												<xs:documentation>KPN ticket identification information.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SRMO_TICKET_ID" type="tns:incidentReportId_type" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Central KPN ticket id, as issued by the ticket management system in the SRMO domain (i.c. Astrid).</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CRM_TICKET_ID" type="tns:incidentReportId_type" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Ticket id as issued by CRM domain when initial CRM ticket is entered in CRM system.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TROUBLE_ID" type="tns:incidentReportId_type" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Identification for related trouble report, if any.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ADDITIONAL_INFORMATION" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Allow free additional information from different sources.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SOURCE" type="xs:string">
														<xs:annotation>
															<xs:documentation>Additional information source.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ADDITIONAL_INFORMATION" type="xs:string">
														<xs:annotation>
															<xs:documentation>Additional information regarding the customer notification as provided by the 'source'.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ANALYSIS_EXPERT_CODE" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The additional information may represent analysis information, for which an optional expert code may indicate a problem class or problem area.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="REPORT_OWNER" type="tns:reportOwner_type" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Identification of the employee accepting the customer notification (request, complaint, report).</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_INITIATION" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Denotes if the incident report was created reactive or proactive.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="EXTERNAL_REFERENCE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Report (ticket) identification in external domain, e.g. customer ticket number (bonnummer klant).</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_RELATED_ID" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The reference can be used to reference other tickets, incidents or other external elements.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_DATETIME" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date and time the message was created i.c. the problem was detected.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_REQUEST_DATE" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Date and time this interaction is to take place. This could be a wish date, planned date etc.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_REQUEST_COMPLETE_DATE" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Date and time issues subject to this interaction are considered closed, not further actions or modifications will be allowed.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ROOT_EVENT_TIME" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date and time of the root event.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_REASON_CODE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This field is used to inform about the reason of the interaction. For example, when a ticket is put on hold, this field can be used to give the reason for the hold action.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_DESCRIPTION_SHORT" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>A short description of the trouble.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_DESCRIPTION_LONG" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>A long description of the trouble.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="IMPACT" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The impact field can be used by the customer to give their opinion on the impact on their business of the communicated change or incident.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="URGENCY" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The customer can use the urgency field to indicate the urgency of this interaction.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="PRIORITY" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The priority field can be used to indicate relative priority in relation to other problem reports.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INCIDENT_CLASS" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>An enumerated field classifying the incident encountered.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TROUBLE_TYPE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The category of the trouble that was detected</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="PERCEIVED_TROUBLE_SEVERITY" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The Perceived Trouble Severity attribute allows the manager to indicate the effect of the trouble on the managed object being reported.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="EXTENDED_INFORMATION" type="xs:string" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="INTERACTION_CLASS" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Denotes the interaction type and subtype. This assists the recipient of the message in interpreting the information.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="INTERACTION_TYPE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Denotes the interaction type. This assists the recipient of the message in interpreting the information.

The current possible values are:
- INCIDENT_REPORT_REQUEST
- CHANGE_REQUEST
- REQUEST_FOR_INFORMATION
- REQUEST_FOR_QUOTATION
- CONFIRMATION
- STATUS_UPDATE
- STATUS_CONFIRMATION
- INTERACTION_UPDATE
Not all values are relevant for all publisher-subscriber interaction processes.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="INTERACTION_SUBTYPE" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Denotes the interaction type subclass. This assists the recipient of the message in interpreting the information.
