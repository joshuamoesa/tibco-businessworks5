<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <name>RequestNotification</name>
    <resourceType>ae.shared.notifySharedConfig</resourceType>
    <config xmlns:xsd="http://www.w3.org/2001/XMLSchema">
        <localOnly>true</localOnly>
        <xsdString>
            <xsd:element name="Request">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="identifier" type="xsd:string"/>
                        <xsd:element name="action" type="xsd:string"/>
                        <xsd:element name="requester" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsdString>
    </config>
</BWSharedResource>