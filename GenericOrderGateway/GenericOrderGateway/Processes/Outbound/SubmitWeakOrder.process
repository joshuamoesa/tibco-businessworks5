                        <ORDER_HEADER>
                            <ORDER_TYPE>
                                <xsl:value-of select="'EXECUTE'"/>
                            </ORDER_TYPE>
                            <COMMERCIAL_PRODUCT_CATEGORY>
                                <xsl:value-of select="$_globalVariables/pfx6:GlobalVariables/GenericOrderGateway/Services/Outbound/SubmitWeakOrder/customerName"/>
