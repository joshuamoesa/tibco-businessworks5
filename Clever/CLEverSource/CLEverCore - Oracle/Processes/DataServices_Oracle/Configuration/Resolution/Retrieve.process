                <pd:x>506</pd:x>
                <pd:y>97</pd:y>
                <config>
                    <processName>/Processes/DataServices_Oracle/Configuration/Resolution/RetrievePossibleParameters.process</processName>
                </config>
                <pd:inputBindings>
