                    </COMPONENT-VERSION>
                    <COPY_ATTRIBUTES>
                        <xsl:value-of select="&quot;true&quot;"/>
                    </COPY_ATTRIBUTES>
                    <EXTENT_TRACKING>
                        <xsl:value-of select="&quot;true&quot;"/>
                    </EXTENT_TRACKING>
                    <xsl:for-each select="$FillCMH/CMH">
