<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>MainStorage</name>
    <resourceType>ae.shared.JDBCSharedResource</resourceType>
    <config>
        <driver>oracle.jdbc.driver.OracleDriver (thin)</driver>
        <maxConnections>%%Connections/DB/Oracle/MainStorage/MaxConnections%%</maxConnections>
        <loginTimeout>%%Connections/DB/Oracle/MainStorage/LoginTimeOut%%</loginTimeout>
        <connectionType>JDBC</connectionType>
        <UseSharedJndiConfig>false</UseSharedJndiConfig>
        <location>jdbc:oracle:thin:@%%Connections/DB/Oracle/MainStorage/Host%%:%%Connections/DB/Oracle/MainStorage/Port%%:%%Connections/DB/Oracle/MainStorage/SID%%</location>
        <password>%%Connections/DB/Oracle/MainStorage/Password%%</password>
        <user>%%Connections/DB/Oracle/MainStorage/UserName%%</user>
    </config>
</BWSharedResource>